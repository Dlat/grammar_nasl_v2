include("compat.inc");

if (description)
{
  script_id(10200);
  script_version("$Revision: 1.28 $");
  script_cvs_date("$Date: 2014/01/14 12:34:31 $");

  script_cve_id("CVE-1999-0271");
  script_osvdb_id(5859);

  script_name(english:"RealServer G2 Malformed Telnet Data Remote Overflow");
  script_summary(english:"Overflows a buffer in RealServer G2");

  script_set_attribute(attribute:"synopsis", value:"Arbitrary code may be run on the remote server.");
  script_set_attribute(attribute:"description", value:
"If a user sends an oversized login / password combination to the
administrative server of a RealServer G2, it will overflow the server's
buffer.

An attacker can use this flaw to execute arbitrary code on the remote
system.");
  script_set_attribute(attribute:"solution", value:
"Filter incoming traffic to the administrative port of the RealServer
G2.");
  script_set_cvss_base_vector("CVSS2#AV:N/AC:L/Au:N/C:C/I:C/A:C");

  script_set_attribute(attribute:"vuln_publication_date", value:"2000/01/15");
  script_set_attribute(attribute:"plugin_publication_date", value:"1999/11/04");

  script_set_attribute(attribute:"plugin_type", value:"remote");
  script_set_attribute(attribute:"cpe", value:"cpe:/a:realnetworks:realserver");
  script_end_attributes();

  script_category(ACT_DESTRUCTIVE_ATTACK);
  script_copyright(english:"This script is Copyright (C) 1999-2014 Tenable Network Security, Inc.");
  script_family(english:"Gain a shell remotely");

  script_dependencies("http_version.nasl");
  script_exclude_keys("global_settings/supplied_logins_only");
  script_require_ports(7070, "Services/realserver");
  exit(0);
}

include("audit.inc");
include("global_settings.inc");
include("misc_func.inc");
include("http.inc");


if (supplied_logins_only) audit(AUDIT_SUPPLIED_LOGINS_ONLY);

port7070 = get_service(svc:"realserver", default:7070, exit_on_fail:TRUE);


#
# A real server is running. Now, let's use the
# result of the port scan to find the admin port
#
if (http_is_dead(port:port7070))exit(0);


admin = 0;
for (i = 1024; i < 65536 && ! admin; i ++)
{
   if(get_port_state(i))
   {
    w = http_send_recv3(method:"GET", item:"/admin/index.html", port:i);
    if (! isnull(w))
    {
      r = strcat(w[0], w[1], '\r\n', w[2]);
      if ("Unauthorized" >< r) admin = i;
    }
   }
}

if (! admin) exit(0, "The administrative server of the RealServer G2 install listening on port "+port7070+" was not found.");


head = "GET /admin/index.html HTTP/1.0\r\n" +
       "Connection: Keep-Alive\r\n" +
       "User-Agent: Mozilla/4.04  (X11; I; Beavuh OS .9 i486; Nav)\r\n"+
       "Host: 111.111.11.1:1111\r\n" +
       "Accept: image/gif, image/x-xbitmap, image/jpeg, image/pjpeg, image/png, */*\r\n"+
       "Accept-Language: en\r\n"+
       "Accept-Charset: iso-8859-1,*,utf-8\r\n"+
       "Authorization: Basic ";
mid = "";
tail = "\r\n\r\n";
str = string(head, mid, tail);
r = http_send_recv_buf(port: admin, data: str);
if (! isnull(r))
{
  sleep(1);
  r2 = http_send_recv3(port: admin, method:"GET", item:"/admin/index.html");
  if (isnull(r2))
  {
    security_hole(admin);
    soc3 = open_sock_tcp(6968);
    if (soc3)
    {
      security_hole(port:6968, extra:"We could open a shell on this port, thanks to the RealG2 overflow.");
      close(soc3);
    }
  }
}
